generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  budgetRange String?
  city        String?
  state       String?
  eventType   String?
  guestCount  Int?
  contact     String?
  notes       String?
  createdAt   DateTime @default(now())
}
